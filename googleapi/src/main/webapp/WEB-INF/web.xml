<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>MainServlet</servlet-name>
  	<display-name>MainServlet</display-name>
  	<servlet-class>com.testing.pdev.controller.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetJSONData</servlet-name>
  	<display-name>GetJSONData</display-name>
  	<servlet-class>com.testing.pdev.controller.GetJSONData</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MainServlet</servlet-name>
  	<url-pattern>/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetJSONData</servlet-name>
  	<url-pattern>/getData</url-pattern>
  </servlet-mapping>
  
</web-app>
